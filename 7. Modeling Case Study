    "df_full['npvis_13_and_above'] = (df_full['npvis'] > 12).astype(int)\n",
    "\n",
    "# Check the DataFrame to see the new columns were created\n",
    "df_full[['npvis_under_10', 'npvis_10_to_12', 'npvis_13_and_above']].head(n=1)"
